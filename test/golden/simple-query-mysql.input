settings {
  engine: "mysql"
}

queries {
  text: "SELECT * FROM users;"
  name: "ListUsers"
  cmd: "SELECT"
  filename: "query/users.sql"
  columns {
    name: "id"
    not_null: true
    type {
      name: "int"
    }
  }
  columns {
    name: "name"
    not_null: true
    type {
      name: "text"
    }
  }
}

global_options: "{ \"cabal_package_name\": \"simple-query-mysql\" }"